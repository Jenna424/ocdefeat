<%= form_tag("/plans", method: "get") do %>
  <%= select_tag "date", options_for_select(["Today", "Past Plans"]), prompt: "Date Designed" %>
  <br><br>
  
  <br><br>
  <%= select_tag "completion", options_for_select(["Finished Plans", "Unfinished Plans"]), prompt: "Progress Toward Completion" %>
  <br><br>
  <%= submit_tag "Filter Plans" %>
<% end %>
